
<%- include('./partial/head') %>
<body>
    <%- include('./partial/navbar') %>
    <section id="header">
        <h1>REGISTER YOUR HOME AND START EARNING</h1>
    </section>
    <section id="main">
        <form id="addhome" action="/host/sucess" method="POST">
            <input type="text" name="Profile" placeholder="ENTER THE PROFILE-IMG LINK" required>
            <br/><br/>
            <input type="text" name="Name" placeholder="ENTER YOUR HOME NAME" required>
            <br/><br/>
            <input type="text" name="Location" placeholder="ENTER YOUR HOME LOCATION" required>
            <br/><br/>
            <input type="number" name="Price" placeholder="ENTER YOUR HOME PRICE PER NIGHT" required>
            <br/><br/>
            <input type="text" name="Ratings" placeholder="ENTER YOUR HOME RATINGS" required>
            <br/><br/>
            <button type="submit">ADD HOME</button>
        </form>
    </section>
    <footer>
        <p>&copy; 2024 Airbnb Clone</p>
    </footer>
</body>
</html>
